#!/bin/bash

########################################################################################################
# File      : ./bin/token-generate                                                                     #
# Author(s) : Zidmann (emmanuel.zidel@gmail.com)                                                       #
# Function  : Script to launch the token generator to create a token for a webservice                  #
# Version   : 1.0.0                                                                                    #
########################################################################################################


CURRENT_DIR=`pwd`
SCRIPT_DIR=`dirname "$0"`

cd $SCRIPT_DIR/../
nodejs ./lib/nodejs/token_generator/generate-token.js
cd $CURRENT_DIR

